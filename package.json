{
  "name": "@hubot-friends/hubot-irc",
  "version": "0.0.0-development",
  "description": "IRC Adapter for Hubot > version `11`.",
  "main": "index.mjs",
  "scripts": {
    "test": "node --test",
    "start": "HUBOT_IRC_ROOMS=#myhubot HUBOT_IRC_SERVER=irc.freenode.net hubot --file ./index.mjs --name irc-hubot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hubot-friends/hubot-irc.git"
  },
  "keywords": [
    "hubot",
    "irc",
    "adapter"
  ],
  "author": "Joey Guerra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hubot-friends/hubot-irc/issues"
  },
  "homepage": "https://github.com/hubot-friends/hubot-irc#readme",
  "peerDependencies": {
    "hubot": ">= 11"
  },
  "engines": {
    "node": ">= 18"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "irc": "^0.5.2"
  }
}
